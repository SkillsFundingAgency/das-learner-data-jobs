{
    "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Trace",
      "Function": "Trace",
      "Host.Results": "Information",
      "Worker": "Trace"
    }
  }
}